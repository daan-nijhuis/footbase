{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ES2021", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "isolatedModules": true,
    "composite": true,
    "outDir": "./_generated",
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["./**/*"],
  "exclude": ["./_generated"]
}
